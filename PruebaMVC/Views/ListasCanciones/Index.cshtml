@model IEnumerable<PruebaMVC.Models.ListasCancione>

@{
    ViewData["Title"] = "Index";
}
<p class="pt-3">
    <a class="gordete" asp-action="Create">Crear Nueva Lista</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Canciones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Listas)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Canciones!.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Listas!.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
