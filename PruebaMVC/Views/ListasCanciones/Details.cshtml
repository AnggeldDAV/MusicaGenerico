@model PruebaMVC.Models.VistaListaCancione

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Listas de Canciones</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Titulo)
        </dt>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Canciones!.Id)


        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Listas!.Id)

        </dd>
    </dl>
</div>
<div>

    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Volver</a>
</div>

