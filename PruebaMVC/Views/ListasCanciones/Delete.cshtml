@model PruebaMVC.Models.ListasCancione

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>¿Está seguro que quiere eliminar la lista?</h3>
<div>
    <h4>Listas de Canciones</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Canciones)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Canciones!.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Listas)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Listas!.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
